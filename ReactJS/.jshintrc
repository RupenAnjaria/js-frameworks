// See https://github.com/jshint/jshint/blob/2.x/examples/.jshintrc
{
    "globals": {
      "window": true,
      "document": true,
      "$": true,
      "XMLHttpRequest": true,
      "chrome": true,
      "Event" : true,
      "React" : true,
      "Q" : true
    },
    "indent": 4,
    "node": true,
    "bitwise": true,
    "camelcase": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "undef": true,
    "unused": true,
    "trailing": true,
    "smarttabs": false,
    "forin": false,
    "browser": false,
    "globalstrict": false,
    "es3": true,
    "esnext": false,
    "quotmark": false

}
